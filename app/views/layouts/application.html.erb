<% if !current_user%>
<%=link_to "Log in", login_path%>
<%=link_to "Sign Up", signup_path %>
<% elsif current_user%>
<%=link_to "#{current_user.username.camelcase}", 'user#show'%>
<%=link_to "Log Out", signout_path, method:'delete' %>
<%=link_to "View Your Recipes", '/'%>
<%=link_to "Upload A Recipe", upload_path%>
<%end%>
<br>
<br>
<%= yield%>